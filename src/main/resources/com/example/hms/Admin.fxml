      <ImageView fx:id="img_admin" fitHeight="72.0" fitWidth="73.0" layoutX="31.0" layoutY="126.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../java/com/example/hms/pics/download%20(2).png" />
