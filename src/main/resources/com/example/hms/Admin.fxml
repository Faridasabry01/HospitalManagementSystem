            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="txtf_password" layoutX="255.0" layoutY="187.0" promptText="Password" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px; -fx-border-color: #7ab1c1;">
